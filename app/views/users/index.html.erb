<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>UserName</th>
      <th>UserEmail</th>
       <th>UserAddress</th>
      <th>User_profile_height</th>
      <th>User_profile_age</th>
      <th>User_profile_gender</th>
      <th colspan="3">Action</th>
    </tr>
  </thead>

  <tbody>
    <% @users. each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.address %></td>
        <td><%= user.profile.userheight %></td>
        <td><%= user.profile.userage %></td>
        <td><%= user.profile.usergender %></td>

        <td><%= link_to 'Show', user %>|</td>
        <td><%= link_to 'Edit_User_Profile', edit_user_path(user) %>|</td>
        <td><%= link_to 'Destroy', user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %>|</td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User create', new_user_path %>